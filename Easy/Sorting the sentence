class Solution:
    def sortSentence(self, s: str) -> str:
        
        s = s.split()
        h={}
        
        for i in s:
            h[int(i[-1])] = i[:-1]
            
        l = len(s)
        output = ''
        for i in range(1,l):
            output = output + h[i] + ' '
        
        return output + h[l]
