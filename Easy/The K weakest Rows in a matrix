class Solution:
    def kWeakestRows(self, mat: List[List[int]], k: int) -> List[int]:
        
        strong = []
        
        for row in mat:
            cnt = 0
            for num in row:
                if num==1:
                    cnt+=1
            
            strong.append(cnt)
        
        output = []
        
        for i in range(k):
            
            temp = strong.index(min(strong))
            
            output.append(temp)
            strong[temp] = len(mat[0]) + 1
        
        return output
