class Solution:
    def canBeIncreasing(self, nums: List[int]) -> bool:
        
        
        def helper(nums:list):
            for i in range(1,len(nums)):
                if nums[i-1]>=nums[i]:
                    return False
            
            return True
        
        h = {}
        
        for i in nums:
            
            if i not in h:
                h[i] = 1
            
            elif h[i]==1:
                h[i]+=1
            
            else:
                return False
        
        for i in range(len(nums)):
            
            temp = nums[:i] + nums[i+1:]
            
            if helper(temp):
                return True
        
        return False
