class Solution:
    def isCovered(self, ranges: List[List[int]], left: int, right: int) -> bool:
        width = []
        for nums in ranges:
            width.extend(list(range(nums[0],nums[1]+1)))
        
        for num in range(left, right+1):
            
            if num not in width:
                return False
        
        return True
