class Solution:
    def twoSumLessThanK(self, nums: List[int], k: int) -> int:
        sums = []
        nums = sorted(nums)
        for i in range(len(nums)):
            
            for j in nums[i+1:]:
                temp = nums[i]+j
                if temp< k:
                    sums.append(temp)
                else:
                    break
        
        return max(sums) if len(sums)>0 else -1
