class Solution:
    def uncommonFromSentences(self, A: str, B: str) -> List[str]:
        
        output = {}
        X = A.split() + B.split()
        
        for i in X:
            if i in output:
                output[i]+=1
            else:
                output[i]=1
        
        result = []
        for key,value in output.items():
            if value==1:
                result.append(key)
        
        return result
