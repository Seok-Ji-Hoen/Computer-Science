class Solution:
    def arrayRankTransform(self, arr: List[int]) -> List[int]:
        if not arr:
            return []
        nums = list(sorted(arr))
        
        h = {nums[0]:1}
        cnt = 2
        for i in range(1,len(nums)):
            
            if nums[i] not in h:
                h[nums[i]] = cnt
                cnt+=1
                
        
        output = []
        
        for i in arr:
            output.append(h[i])
            
        return output
