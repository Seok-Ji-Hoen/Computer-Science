class Solution:
    def countAndSay(self, n: int) -> str:
        
        if n==1:
            return '1'
        
        else:
            curr = ''
            prev = self.countAndSay(n-1)
            cnt = 1
            for i in range(len(prev)-1):
                if prev[i]==prev[i+1]:
                    cnt+=1
                
                else:
                    curr+=str(cnt)+prev[i]
                    cnt=1
            curr+=str(cnt)+prev[-1]
            return curr
