class Solution:
    def canBeTypedWords(self, text: str, brokenLetters: str) -> int:
        
        text = text.split()
        cnt = 0
        
        for word in text:
            
            write = True
            
            for i in brokenLetters:
                
                if i in word:
                    write = False
                    
            
            if write:
                cnt+=1
        return cnt
